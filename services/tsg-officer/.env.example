# Optional: choose which LLM backend to use
# mock | openai | chubbgpt
TSG_LLM_PROVIDER=mock

# Optional (openai provider)
OPENAI_API_KEY=
TSG_OPENAI_MODEL=gpt-4o-mini

# Optional (chubbgpt provider)
# These are required when TSG_LLM_PROVIDER=chubbgpt
TSG_CHUBBGPT_PROXY_URL=
TSG_CHUBBGPT_APP_ID=
TSG_CHUBBGPT_APP_KEY=
TSG_CHUBBGPT_RESOURCE=

# Optional overrides
TSG_CHUBBGPT_AUTH_URL=https://studiogateway.chubb.com/enterprise.operations.authorization?Identity=AAD
TSG_CHUBBGPT_API_VERSION=1
TSG_CHUBBGPT_MODEL=

# dual-model routing (the key part)
TSG_CHUBBGPT_CHECKLIST_MODEL=gpt-5.1
TSG_CHUBBGPT_REASONING_MODEL=gpt-4o

# Path to local sqlite file used by the checkpointer
TSG_CHECKPOINT_DB=./.tsg_checkpoints.sqlite
